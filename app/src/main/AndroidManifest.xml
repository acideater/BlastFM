<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.blastfm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GarageActivity" android:label="@string/activity_garage" />
        <activity android:name=".PunkActivity" android:label="@string/activity_punk" />
        <activity android:name=".PsychedelicActivity" android:label="@string/activity_psychedelic" />
        <activity android:name=".NowPlayingActivity" android:label="@string/now_playing" />
    </application>

</manifest>